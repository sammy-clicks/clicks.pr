{
  "name": "clicks-v1",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/predeploy_check.js",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "render:build": "npm run db:generate && npm run build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "create:admin": "tsx scripts/create_admin.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "jose": "^5.9.3",
    "next": "^14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.13",
    "prisma": "^5.20.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  }
}